  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="768.0" prefWidth="1366.0" VBox.vgrow="ALWAYS">
      <children>
            <Canvas fx:id="canvas" height="768.0" layoutX="1.0" layoutY="26.0" width="1366.0" />
            <Button fx:id="shapeButton" layoutX="1.0" mnemonicParsing="false" onAction="#drawShapesClick" prefHeight="29.0" prefWidth="121.0" text="Draw Shapes" />
      </children>
    </AnchorPane>
  </children>
</VBox>
